{
  "name": "symfony-orchestra/view-bundle",
  "type": "library",
  "description": "The view symfony bundle for JSON API responses",
  "keywords": [
    "symfony",
    "view-bundle",
    "view",
    "api",
    "json",
    "typescript"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Lukin",
      "email": "lukin.andrej@gmail.com",
      "homepage": "https://github.com/wtorsi",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.4",
    "symfony/http-kernel": "7.2.*",
    "symfony/serializer": "7.2.*",
    "symfony/property-access": "7.2.*",
    "symfony/dependency-injection": "7.2.*",
    "symfony/config": "7.2.*",
    "doctrine/common": "^3.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "SymfonyOrchestra\\ViewBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.2.*"
    }
  }
}
